{
  "name": "three-typescript-webpack-starter",
  "version": "1.0.0",
  "description": "A starter project for using Three.js, TypeScript, SCSS and Webpack.",
  "main": "index.js",
  "scripts": {
    "dev": "webpack serve --mode development --progress --hot --config ./webpack.config.development.js",
    "dev:secure": "webpack serve --https --mode development --progress --hot --config ./webpack.config.development.js",
    "build": "webpack --mode production --config ./webpack.config.production.js",
    "test": "jest",
    "test:mutation": "npx stryker run",
    "start": "http-server dist"
  },
  "keywords": [
    "three.js",
    "typescript",
    "webpack",
    "scss"
  ],
  "author": "Grant Bartlett",
  "license": "MIT",
  "dependencies": {
    "d3": "^7.4.0",
    "lil-gui": "^0.15.0",
    "three": "^0.134.0"
  },
  "devDependencies": {
    "@stryker-mutator/core": "^6.0.2",
    "@stryker-mutator/jest-runner": "^6.0.2",
    "@stryker-mutator/typescript-checker": "^6.0.2",
    "@types/d3": "^7.1.0",
    "@types/jest": "^27.0.3",
    "@types/three": "^0.134.0",
    "@webpack-cli/serve": "^1.6.0",
    "clean-webpack-plugin": "^4.0.0",
    "copy-webpack-plugin": "^10.0.0",
    "css-loader": "^6.5.1",
    "esm": "^3.2.25",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.4.3",
    "mini-css-extract-plugin": "^2.4.5",
    "npx": "^10.2.2",
    "postcss": "^8.4.4",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.0.1",
    "sass": "^1.44.0",
    "sass-loader": "^12.3.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.2.5",
    "ts-jest": "^27.1.2",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.2",
    "webpack": "^5.64.4",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.9.0",
    "workbox-webpack-plugin": "^6.5.2"
  }
}
